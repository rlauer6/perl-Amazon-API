  my ( $class, @options ) = @_;
  $class = ref($class) || $class;

  my %options = ref( $options[0] ) ? %{ $options[0] } : @options;

  $class->SUPER::new(
    { service     => 'ec2',
      version     => '2016-11-15',
      api_methods => \@API_METHODS,
      %options,
    }
  );
} ## end sub new

